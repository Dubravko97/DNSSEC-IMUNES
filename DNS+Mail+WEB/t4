 n9 {
    type router
    cpu {{min 0} {max 100} {weight 1}}
    model quagga
    network-config {
	hostname R9
	!
	interface lo0
	 type lo
	 ip address 127.0.0.1/24
	 ipv6 address ::1/128
	!
	interface eth2
	 ip address 10.0.10.1/24
	!
	interface eth1
	 ip address 10.0.9.1/24
	!
	interface eth0
	 ip address 10.0.8.1/24
	!
	interface eth4
	 ip address 10.0.7.2/24
	!
	router rip
	 redistribute static
	 network 0.0.0.0/0
	!
	!
